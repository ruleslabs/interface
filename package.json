{
  "name": "@rulesorg/interface",
  "version": "1.0.0",
  "description": "Rules interface",
  "scripts": {
    "prei18n:extract": "touch src/locales/en-US.po",
    "i18n:extract": "NODE_ENV=development lingui extract --locale en-US",
    "i18n:compile": "yarn i18n:extract && lingui compile",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "prepare": "yarn i18n:compile",
    "build": "yarn run clean && next build",
    "clean": "rm -rf .next",
    "lint": "next lint",
    "start": "next start",
    "dev": "env-cmd -f .env.local next dev",
    "build:dev": "env-cmd -f .env.development yarn run build",
    "build:prod": "env-cmd -f .env.production yarn run build",
    "deploy:dev": "yarn run build:dev && rm -rf .serverless .serverless_nextjs && env-cmd -f .env.development components-v1 --component",
    "deploy:prod": "yarn run build:prod && rm -rf .serverless .serverless_nextjs && env-cmd -f .env.production components-v1 --component"
  },
  "author": "Charles Lanier",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@apollo/react-ssr": "^4.0.0",
    "@ethersproject/contracts": "^5.6.0",
    "@ethersproject/providers": "^5.6.5",
    "@ethersproject/units": "^5.6.0",
    "@lingui/core": "^3.13.3",
    "@lingui/react": "^3.13.3",
    "@ramp-network/ramp-instant-sdk": "^3.0.0",
    "@reach/dialog": "^0.16.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@rulesorg/sdk-core": "^0.8.11",
    "@rulesorg/starknet-redux-multicall": "0.1.1",
    "@stripe/react-stripe-js": "^1.7.1",
    "@stripe/stripe-js": "^1.27.0",
    "@svgr/webpack": "^6.1.2",
    "@web3-react/core": "^8.0.20-beta.0",
    "@web3-react/metamask": "^8.0.16-beta.0",
    "algoliasearch": "^4.12.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "bn.js": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "crypto-hash": "^2.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "js-levenshtein": "^1.1.6",
    "moment": "^2.29.1",
    "next": "^12.1.0",
    "otplib": "^12.0.1",
    "qrcode.react": "^3.1.0",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-redux": "^7.2.6",
    "react-spring": "^9.3.2",
    "redux-localstorage-simple": "^2.5.1",
    "starknet": "^3.18.2",
    "styled-components": "^5.3.3",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@lingui/cli": "^3.13.3",
    "@lingui/loader": "^3.13.3",
    "@lingui/macro": "^3.13.3",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/bn.js": "^5.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/js-levenshtein": "^1.1.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.35",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "env-cmd": "^10.1.0",
    "eslint": "<8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "serverless-next.js": "^1.15.0-alpha.2",
    "typescript": "^4.5.4"
  }
}
